  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "email.php",
      "default_title": "privacy preserving authentication"
       },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}

